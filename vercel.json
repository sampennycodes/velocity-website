{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "dist/server/entry.mjs": {
      "runtime": "nodejs22.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/server/entry.mjs"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/server/entry.mjs"
    }
  ]
}
